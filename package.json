  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:nextjs": "next dev",
    "typescript": "tsc -w --pretty --preserveWatchOutput",
    "typescript1": "tsc --pretty --preserveWatchOutput",
